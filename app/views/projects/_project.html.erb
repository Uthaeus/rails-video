<div id="<%= dom_id project %>" class='col'>
  <div class='item-card'>
        <div class='item-card-image'>
            <%= image_tag "#{project.thumbnail}", width: '100%', class: 'item-card-image' %>
        </div>
        <div class='item-card-content'>
            <h3><%= link_to project.title, project_path(project) %></h3>
            <p><%= project.status %></p>
        </div>
    </div>
</div>
